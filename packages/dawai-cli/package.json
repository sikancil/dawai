{
  "name": "@arifwidianto/dawai-cli",
  "version": "0.1.0",
  "description": "CLI scaffolding tool for the Dawai framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dawai": "dist/bin/dawai.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "start": "node dist/bin/dawai.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@arifwidianto/dawai-common": "0.1.0",
    "@arifwidianto/dawai-microservice": "0.1.0",
    "@arifwidianto/dawai-stdio": "0.1.0",
    "fs-extra": "^11.0.0",
    "chalk": "^4.1.2",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^20.11.19",
    "@types/fs-extra": "^11.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
