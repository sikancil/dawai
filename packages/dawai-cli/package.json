{
  "name": "@arifwidianto/dawai-cli",
  "version": "0.1.0",
  "description": "CLI scaffolding tool for the Dawai framework",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dawai": "dist/bin/dawai.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
