    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/minimist": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
