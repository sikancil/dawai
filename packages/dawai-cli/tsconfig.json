{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    // "rootDir": ".",
    // "composite": true,
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "module": "nodenext",
    "target": "es2022",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ES2022"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"],
  "references": []
}
